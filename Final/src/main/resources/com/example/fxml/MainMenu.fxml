<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@../../../css/MainMenuTheme.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.Final.FXMLController.MainMenuController">
   <children>
      <HBox layoutX="250.0" layoutY="307.0" prefHeight="80.0" prefWidth="300.0" spacing="140.0">
         <children>
            <Button fx:id="loginButton" mnemonicParsing="false" onAction="#goToLoginMenu" prefHeight="80.0" prefWidth="80.0" text="login" />
            <Button fx:id="registerButton" mnemonicParsing="false" onAction="#goToRegisterMenu" prefHeight="80.0" prefWidth="80.0" text="register" />
         </children>
      </HBox>
      <VBox layoutX="200.0" layoutY="32.0" prefHeight="180.0" prefWidth="400.0" spacing="40.0">
         <children>
            <Label fx:id="mainMenuLabel" alignment="CENTER" prefHeight="100.0" prefWidth="400.0" text="TapsiFood !">
               <font>
                  <Font name="Cambria Bold Italic" size="12.0" />
               </font>
            </Label>
            <Label fx:id="welcomeLabel" alignment="CENTER" prefHeight="30.0" prefWidth="400.0" text="Welcome Fella !" />
         </children>
      </VBox>
   </children>
</AnchorPane>
