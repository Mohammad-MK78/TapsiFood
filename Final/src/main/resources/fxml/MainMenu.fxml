<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@../css/MainMenuTheme.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.Final.FXMLController.FXMainMenuController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="881.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Pictures/MainMenu.png" />
         </image>
      </ImageView>
      <HBox layoutX="237.0" layoutY="300.0" prefHeight="97.0" prefWidth="350.0" spacing="120.0">
         <children>
            <Button fx:id="loginButton" mnemonicParsing="false" onAction="#goToLoginMenu" prefHeight="100.0" prefWidth="100.0" text="login" />
            <Button fx:id="registerButton" mnemonicParsing="false" onAction="#goToRegisterMenu" prefHeight="100.0" prefWidth="100.0" text="register" />
         </children>
      </HBox>
   </children>
</AnchorPane>
